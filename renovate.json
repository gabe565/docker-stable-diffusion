{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>gabe565/renovate-config"
  ],
  "regexManagers": [
    {
      "fileMatch": ["Dockerfile", "^\\.github/workflows/", "README.md"],
      "matchStrings": [
        "\\s*SD_WEBUI_REPO: +(?<depName>.+)\\n\\s*SD_WEBUI_REF: +(?<currentValue>.+)",
        "ARG +SD_WEBUI_REPO=(?<depName>.+)\\nARG +SD_WEBUI_REF=(?<currentValue>.+)",
        "<!--renovate repo=(?<depName>.+) -->\\n.*\\/Version-(?<currentValue>.+?)-"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
